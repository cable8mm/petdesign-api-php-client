{
    "name": "cable8mm/petdesign-api-php-client",
    "type": "library",
    "description": "Client library for Petdesign APIs",
    "keywords": [
        "petdesign"
    ],
    "homepage": "https://palgle.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "./vendor/bin/phpunit tests"
        ]
    }
}